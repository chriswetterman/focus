{
    "name": "Focus",
    "version": "1.0",
    "description": "Stay focused",
    "manifest_version": 2,
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["content.js"],
        "matches": ["http://*/*", "https://*/*"]
      }],
      "content_security_policy": "script-src 'self' https://github.com; object-src 'self'"
  }